/* c program to print a statement on output screen */
#include<stdio.h>
void main()
{
/* taking input of two numbers */
int a,b,c;
c=0;
printf("enter the first number");
scanf("%d",&a);
printf("enter the second number");
scanf("%d",&b);
/*logic for adding two numbers*/;
c=a+b;
/*display the output*/
printf("the result of additon of two numbers is %d",c);
printf("raghu is coder of anits 2/4 csea");
}

